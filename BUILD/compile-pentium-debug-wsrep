#! /bin/sh -x

path=`dirname $0`
set -- "$@" --with-debug=full
. "$path/SETUP.sh"

extra_flags="$pentium_cflags $debug_cflags -g -O0"
c_warnings="$c_warnings $debug_extra_warnings"
cxx_warnings="$cxx_warnings $debug_extra_warnings"
extra_configs="$pentium_configs $debug_configs --with-wsrep"

. "$path/FINISH.sh"
